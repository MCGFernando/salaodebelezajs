<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('../partials/head.ejs') %>
    <title>Document</title>
  </head>
  <body>
    
    <%- include('../partials/sidebar.ejs') %>
    <%- include('../partials/header.ejs') %>
    <div class="container">
      <form action="" class="form-container">
        <div class="input-container">
          <label for="sequenciaCategoria">Sequência</label>
          <input
            type="text"
            id="sequenciaCategoria"
            name="sequenciaCategoria"
            required
            class="input-field"
            placeholder="Digite a sequência"
          />
        </div>
          
          
        
        <div class="input-container">
          <label for="categoria">Categoria</label>
          <input type="text" id="categoria" name="categoria" required class="input-field" placeholder="Digite a categoria"/>
        </div>

        <div class="form-btn-container">
          <input type="button" value="Cadastrar">
          <input type="reset" value="Limpar">
        </div>
      </form>
    </div>
    <%- include('../partials/footer.ejs') %>
  </body>
</html>
