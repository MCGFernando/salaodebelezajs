<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/mystyle.css" />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
    />
    <title>Document</title>
  </head>
  <body>
    <%- include('../partials/head.ejs') %>
     <%- include('../partials/sidebar.ejs') %>
    <main class="main">
      <div class="card">
        <div class="card-header">
          <h2 class="card-title">Formulário de Cadastro - Categorias</h2>
        </div>
        <div class="card-body">
          <form action="" class="form-container">
            <div class="form-group">
              <label for="sequenciaCategoria">Sequência</label>
              <input
                type="text"
                id="sequenciaCategoria"
                name="sequenciaCategoria"
                required
              />
            </div>
            <div class="form-group">
              <label for="categoria">Categoria</label>
              <input type="text" id="categoria" name="categoria" required />
            </div>

            <div class="form-btn-container">
              <button class="btn btn-submit">Cadastrar</button>
              <button type="reset" class="btn btn-reset">Limpar</button>
            </div>
          </form>
        </div>
      </div>
    </main>
  </body>
</html>
